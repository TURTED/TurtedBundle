{
  "name": "turted/turted-bundle",
  "description": "Symfony Bundle to send Push messages to a TURTED server",
  "type": "vcs",
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Dietrich",
      "email": "pd@xosofox.de",
      "homepage": "https://xosofox.de",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Turted\\TurtedBundle\\": "src/"
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "require": {
    "php": "^5.5.3 || ^7.0",
    "symfony/framework-bundle": "^4.0 || ^5.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5 || ^7.0 || ^8.0 || ^9.0"
  }
}
